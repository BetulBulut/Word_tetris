<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".game_end">

    <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/t1"
            android:layout_marginLeft="100dp"
            android:layout_marginTop="25dp"
            android:layout_marginRight="100dp"
            android:textAlignment="center"
            android:textColor="@android:color/holo_blue_dark" android:textSize="75sp"
            tools:ignore="MissingConstraints"/>
    <TextView
            android:layout_marginLeft="100dp"
            android:layout_marginTop="100dp"
            android:layout_marginRight="100dp"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:textColor="@android:color/holo_blue_dark" android:textSize="50sp"
            android:layout_height="wrap_content" android:id="@+id/t2"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_marginLeft="100dp"
            android:layout_marginTop="150dp"
            android:layout_marginRight="100dp"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/t3"
            android:textColor="@android:color/holo_blue_dark" android:textSize="50sp"

            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_marginLeft="100dp"
            android:layout_marginTop="200dp"
            android:layout_marginRight="100dp"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/t4"
            android:textColor="@android:color/holo_blue_dark" android:textSize="50sp"

            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_marginLeft="100dp"
            android:layout_marginTop="250dp"
            android:layout_marginRight="100dp"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/t5"
            android:textColor="@android:color/holo_blue_dark" android:textSize="50sp"

            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_marginLeft="100dp"
            android:layout_marginTop="300dp"
            android:layout_marginRight="100dp"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/t6"
            android:textColor="@android:color/holo_blue_dark" android:textSize="50sp"

            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_marginLeft="100dp"
            android:layout_marginTop="350dp"
            android:layout_marginRight="100dp"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/t7"
            android:textColor="@android:color/holo_blue_dark" android:textSize="50sp"

            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
    <TextView
            android:layout_marginLeft="100dp"
            android:layout_marginTop="400dp"
            android:layout_marginRight="100dp"
            android:textAlignment="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/t8"
            android:textColor="@android:color/holo_blue_dark" android:textSize="50sp"

            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>